{
  "name": "basic_config",
  "seed": 42,
  "env_type":
  {
    "n_row": 5,
    "n_col": 4,
    "state_type" : "surrounding",
    "change_maze" : 0,
    "maze_type" : "sequential",
    "objective" : {
      "type" : "image_no_bkg",
      "curriculum":
      {
        "n_objective" : 15,
        "change_every" : 10
      }

    }
  },
  "train_params": {
    "epsilon_schedule": ["exp", 0.95],
    "batch_size": 64,
    "n_epochs" : 2000,
    "test_every": 100,
    "n_epochs_test": 7
  },
  "io": {
    "verbosity": 1,
    "num_epochs_to_store": 2
  },

  "dqn_params":
  {
    "conv_shapes": [16],
    "dense_shapes": [],
    "learning_rate" : 1e-5,
    "optimizer": "RMSprop",
    "use_batch_norm": false,
    "gamma": 0.99,
    "tau" : 0.0001
  },

  "reinforce_params": {
    "conv_shapes": [16],
    "dense_shapes": [],
    "learning_rate" : 1e-5,
    "optimizer": "Adam",
    "use_batch_norm": false,
    "gamma": 0.99,
    "update_every": 2,
    "concatenate_objective" : true
  },

  "resnet_dqn_params":{
    "learning_rate" : 1e-5,
    "optimizer": "RMSprop",
    "gamma": 0.99,
    "n_resblock" : 2,
    "n_hidden" : 200,
    "use_film" : false,
    "tau" : 0.0001
  }

}
