{
  "name": "dqn_filmed",

  "agent_type":"resnet_dqn",
  "images_features" : "normalized",

  "resnet_dqn_params":{
    "film_gen_param_vision":
      {
        "n_intermediate_channel": 16,
        "intermediate_kernel_size": 5,

        "n_final_channel": 32,
        "final_kernel_size": 5,

        "n_hidden_gamma" : 400,
        "n_hidden_beta" : 400,
        "dropout" : 0
      },

    "film_gen_param_text":
    {
      "n_hidden_gamma" : 200,
      "n_hidden_beta" : 200,
      "dropout" : 0
    },


    "learning_rate" : 1e-5,
    "batch_size" : 64,
    "optimizer": "rmsprop",
    "discount_factor": 0.90,

    "n_modulated_block": 4,
    "n_regular_block": 0,

    "resblock_dropout": 0,
    "fc_dropout": 0,
    "n_hidden" : 600,

    "head_kernel": 1,
    "head_pool_kernel": 1,
    "head_channel": 1,
    "word_emb_size" : 20,
    "lstm_size" : 200,


    "use_film" : true,

    "tau" : 0.01,
    "soft_update" : true
  }
}
