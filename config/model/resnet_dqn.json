{
  "name": "resnet_dqn",

  "agent_type":"resnet_dqn",
  "images_features" : "normalized",

  "resnet_dqn_params":{

    "learning_rate" : 1e-5,
    "batch_size" : 64,
    "optimizer": "RMSprop",
    "discount_factor": 0.90,

    "n_modulated_block": 0,
    "n_regular_block": 4,

    "resblock_dropout": 0,
    "head_kernel" : 1,
    "head_pool_kernel" : 1,
    "head_channel" : 1,

    "fc_dropout" : 0,
    "n_hidden" : 400,

    "word_emb_size" : 30,
    "lstm_size" : 200,

    "use_film" : false,
    "tau" : 0.1,
    "soft_update" : false
  }

}
