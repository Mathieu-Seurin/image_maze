{
  "name": "resnet_dqn",

  "agent_type":"resnet_dqn",
  "images_features" : "normalized",

  "resnet_dqn_params":{

    "learning_rate" : 1e-5,
    "batch_size" : 64,
    "optimizer": "RMSprop",
    "discount_factor": 0.90,

    "n_modulated_block": 0,
    "n_regular_block": 2,

    "resblock_dropout": 0,
    "fc_dropout" : 0,
    "n_hidden" : 400,

    "head_kernel": 3,
    "head_pool_kernel": 2,
    "head_channel": 3,


    "word_emb_size" : 30,
    "lstm_size" : 200,

    "use_film" : false,
    "tau" : 0.001,
    "soft_update" : true
  }

}
