    "resblock_dropout": 0,
    "fc_dropout": 0,
    "n_hidden" : 400,

    "use_film" : false,
    "tau" : 0.1,
    "soft_update" : false,


    "head_kernel": 1,
    "head_pool_kernel": 1,
    "head_channel": 10,
    "word_emb_size" : 30,
    "lstm_size" : 200

  }
