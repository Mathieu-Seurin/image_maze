{
  "name": "reinforce_filmed_pretrain",

  "agent_type":"resnet_reinforce",
  "pretrained_features" : true,

  "resnet_reinforce_params":{
    "film_gen_params":
      {
        "n_intermediate_channel": 16,
        "intermediate_kernel_size": 5,

        "n_final_channel": 32,
        "final_kernel_size": 5,

        "n_hidden_gamma" : 400,
        "n_hidden_beta" : 400,
        "dropout" : 0
      },
    "learning_rate" : 1e-3,
    "optimizer": "rmsprop",
    "discount_factor": 0.9,
    "n_modulated_block": 2,
    "n_regular_block": 0,
    "resblock_dropout": 0,
    "fc_dropout": 0,
    "n_hidden" : 400,
    "head_channel" : 1,
    "head_kernel" : 1,
    "head_pool_kernel" : 2,
    "use_film" : true,
    "update_every": 100,
    "concatenate_objective" : false
  }
}
