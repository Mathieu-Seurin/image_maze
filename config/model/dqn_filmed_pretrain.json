{
  "name": "dqn_filmed_pretrain",

  "agent_type":"resnet_dqn",

  "images_features" : "specific",

  "resnet_dqn_params":{

    "film_gen_param_vision":
      {
        "n_intermediate_channel": 16,
        "intermediate_kernel_size": 0,

        "n_final_channel": 32,
        "final_kernel_size": 5,

        "n_hidden_gamma" : 400,
        "n_hidden_beta" : 400,
        "dropout" : 0
      },

     "film_gen_param_text":
    {
      "film_type" : "simple",
      "use_feedback" : false,
      "film_gen_hidden_size" : 512,
      "film_attention_size_hidden" : 200,
      "vision_reducing_size_mlp" : 40,
      "vision_reducing_method" : "mlp"
    },

    "learning_rate" : 1e-5,
    "batch_size" : 64,

    "optimizer": "rmsprop",

    "default_w_decay" : 0,
    "FiLM_decay" : 0,

    "discount_factor": 0.90,

    "n_modulated_block": 4,
    "n_regular_block": 0,

    "resblock_dropout": 0,
    "fc_dropout": 0,
    "n_hidden" : 400,

    "head_kernel": 3,
    "head_pool_kernel": 2,
    "head_channel": 3,

    "word_emb_size" : 20,
    "lstm_size" : 200,

    "use_film" : true,
    "fusing_method" : "no_fuse",

    "hidden_mlp_attention": 0,
    "embedding_size_before_dot" : 0,

    "memory_size" : 10000,

    "exploration_method" : "eps_greedy",

    "tau" : 0.001,
    "soft_update" : true
  }
}
