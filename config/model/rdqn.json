{
  "name": "rdqn",

  "agent_type":"rdqn",
  "images_features" : "normalized",

  "rdqn_params":{

    "learning_rate" : 1e-5,
    "batch_size" : 64,

    "optimizer": "RMSprop",
    "default_w_decay" : 0,

    "discount_factor": 0.90,

    "n_modulated_block": 0,
    "n_regular_block": 2,

    "resblock_dropout": 0,
    "fc_dropout" : 0,

    "head_kernel": 3,
    "head_pool_kernel": 2,
    "head_channel": 3,

    "use_film" : false,
    "fusing_method_before_recurrent" : "concatenate",
    "fusing_method_after_recurrent" : "no_fuse",

    "n_hidden_lstm_dyn" : 300,
    "lstm_dyn_n_layer" : 1,

    "embedding_size_before_dot" : 0,
    "hidden_mlp_attention": 0,

    "word_emb_size" : 30,
    "text_lstm_size" : 200,

    "memory_size" : 2000,
    "max_seq_length" : 12,
    "tau" : 0.001,
    "soft_update" : true
  }

}
