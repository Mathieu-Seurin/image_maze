{
  "name": "reinforce",

  "train_params": {
    "batch_size": 64
  },

  "agent_type":"reinforce",
  "image_features" : "normalized",

  "reinforce_params": {
    "input_resolution": [28, 28],
    "n_channels": 18,
    "conv_shapes": [16, 16],
    "dense_shapes": [64],
    "learning_rate" : 1e-3,
    "optimizer": "rmsprop",
    "use_batch_norm": true,
    "discount_factor": 0.99,
    "update_every": 50,
    "concatenate_objective" : true,
    "use_first_block": false
  }

}
